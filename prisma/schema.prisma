generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = "postgres://default:peu5JXnNbLA4@ep-lucky-frog-a4w2o2za.us-east-1.aws.neon.tech/verceldb?sslmode=require"
